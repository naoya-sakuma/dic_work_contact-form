<%= form_with(model: @album, local: true) do |f| %>
<table class="album_new_table">
  <tr>
  <th><%= f.label :name, "name" %></th>
  <th><%= f.text_field :name %></th>
  </tr>
<tr>
  <th><%= f.label :email, "email" %></th>
  <th><%= f.email_field :email %></th>
</tr>
<tr>
  <th><%= f.label :content, "content" %></th>
  <th><%= f.text_field :content %></th>
</tr>

<tr>
  <th></th>
  <th id= "make_account" ><%= f.submit "送信" %></th>
  </tr>
<% end %>
